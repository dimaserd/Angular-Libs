{
  "name": "croco-html",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "run:croco-html-sprites": "node projects/croco-html/src/lib/tools/build-sprites.js",
    "build:croco-generic-interface-components": "ng build croco-generic-interface-components",
    "build:angular-js-open-api": "ng build angular-js-open-api",
    "build:croco-generic-app-logic": "ng build croco-generic-app-logic",
    "build:croco-html": "npm run run:croco-html-sprites && ng build croco-html",
    "build:croco-type-description": "ng build croco-type-description",
    "build:tutor-logic": "ng build tutor-logic",
    "watch": "ng build croco-html --watch --configuration development",
    "watch:croco-generic-interface-components": "ng build croco-generic-interface-components --watch --configuration development",
    "test": "ng test",
    "build-all": "npm run build:angular-js-open-api && npm run build:croco-generic-app-logic && npm run build:croco-html && npm run build:croco-type-description && npm run build:croco-generic-interface-components && npm run build:tutor-logic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@ng-select/ng-select": "^15.1.3",
    "ngx-clipboard": "^16.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.15.1",
    "croco-generic-app-logic": "5.0.0",
    "tutor-logic": "5.0.0",
    "date-fns": "^2.21.1",
    "ngx-scrollbar": "16.1.1",
    "ngx-skeleton-loader": "^7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.2",
    "@angular/cli": "^20.1.2",
    "@angular/compiler-cli": "^20.1.3",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^18.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^20.1.0",
    "svg-sprite": "^2.0.2",
    "typescript": "~5.8.3"
  }
}
