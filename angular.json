            "production": {
              "tsConfig": "projects/croco-html/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/croco-html/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "main": "projects/croco-html/src/test.ts",
            "tsConfig": "projects/croco-html/tsconfig.spec.json",
            "karmaConfig": "projects/croco-html/karma.conf.js"
          }
        }
      }
    },
    "ecc-chat": {
      "projectType": "library",
      "root": "projects/ecc-chat",
      "sourceRoot": "projects/ecc-chat/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ecc-chat/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ecc-chat/tsconfig.lib.prod.json"
