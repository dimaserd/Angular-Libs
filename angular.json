          }
        }
      }
    },
    "croco-generic-app-logic": {
      "projectType": "library",
      "root": "projects/croco-generic-app-logic",
      "sourceRoot": "projects/croco-generic-app-logic/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/croco-generic-app-logic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/croco-generic-app-logic/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/croco-generic-app-logic/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/croco-generic-app-logic/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "tutor-logic": {
